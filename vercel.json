{
    "version": 2, 
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            },
            {
              "key": "Access-Control-Allow-Methods",
              "value": "GET,HEAD,PUT,PATCH,POST,DELETE"
            },
            {
              "key": "Access-Control-Allow-Headers",
              "value": "Content-Type"
            }
          ]
        }
      ]
} 